<application id="org.gtk.example">
<applicationwindow title="Window" height="200" width="200">
    <?script
        function print_hello (GtkWidget $widget, $data):void {
            echo "Hello $data\n";
        }
    ?>
    <buttonbox>
        <button label="Hello World" clicked="print_hello($../../button$, 'World'); gtk_widget_destroy($_GLOBAL['window']);"/>
    </buttonbox>
</applicationwindow>
</application>
